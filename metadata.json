{
  "name": "puppet-erlang",
  "version": "1.1.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Manages repos and package Erlang installation",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-erlang",
  "project_page": "https://github.com/voxpupuli/puppet-erlang",
  "issues_url": "https://github.com/voxpupuli/puppet-erlang/issues",
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 2.0.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
